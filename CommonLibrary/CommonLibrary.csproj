<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>laz-the-developer-spicy-wing</PackageId>
    <Title>Spicy Wing</Title>
    <PackageTags>LazLibrary2015; Spicy Wing</PackageTags>
    <PackageReleaseNotes>Simple and Why Even Use it</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/lazuhrow93/CommonLibrary</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Update="license.txt">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
